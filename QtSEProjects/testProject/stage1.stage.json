{
    "itemName": "stage1.stage",
    "itemType": "stage",

    "generatePerspectiveMatrixPerspective": {
        "fovy": 80.0,
        "aspect": {
            "windowWidth": 1.0,
            "windowHeight": 1.0
        },
        "zNear": 0.1,
        "zFar": 100.0
    },

    "viewTransforms": [
        {
            "operation": "loadIdentity"
        },
        {
            "operation": "rotate",
            "arguements": [ 45.0, 0.0, 0.0, 1.0 ]
        },
        {
            "operation": "translate",
            "arguments": [ 1.0, 1.0, 1.0 ]
        }
    ],

	"inputFramebufferList": [
		"framebuffer1.framebuffer"
	],

	"outputFrameBuffer": "context",

    "modelList": [
        "model1.model"
    ],

	"runOnce": false
}

